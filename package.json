{
  "name": "sensei-sensor-react",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write ./**/*.{js,jsx,html,css,scss,md,json}",
    "lint": "eslint ./src --ext .js,.jsx",
    "lint:fix": "eslint --fix ./src --ext .js,.jsx",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "lint:style:fix": "stylelint --fix '**/*.{css,scss,sass}'",
    "fix": "npm run format && npm run lint:fix && npm run lint:style:fix",
    "prepare": "node --eval 'process.exit(process.env.NODE_ENV === `production` ? 0 : 1)' || husky install",
    "lint-staged": "lint-staged",
    "gcp-build": "vite build",
    "deploy": "gcloud app deploy"
  },
  "lint-staged": {
    "*.{js,jsx,json,html,md,json}": [
      "prettier --write ."
    ],
    ".{js,jsx}": [
      "eslint --fix ."
    ],
    "**/*.{css,scss,sass}": [
      "stylelint --fix ."
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.4",
    "@mui/material": "^5.0.4",
    "@mui/styles": "^5.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "axios": "^0.24.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.4",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^2.6.10"
  }
}
